{
	"compilerOptions": {
		"target": "es2016",
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "node",
		"baseUrl": ".",
		"paths": {
			"*": ["*", "e2e/*"]
		},
		"resolveJsonModule": true
	},
	"include": ["e2e/**/*", "*.ts"],
	"exclude": ["node_modules"]
}
